{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/css/(.*)",
            "destination": "/public/css/$1"
        },
        {
            "source": "/js/(.*)",
            "destination": "/public/js/$1"
        },
        {
            "source": "/images/(.*)",
            "destination": "/public/images/$1"
        },
        {
            "source": "/uploads/(.*)",
            "destination": "/public/uploads/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.js"
        }
    ]
}